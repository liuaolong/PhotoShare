<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.notifications.NotificationsFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">

        <!--        <ImageView-->
        <!--            android:id="@+id/touxiang"-->
        <!--            android:layout_width="95dp"-->
        <!--            android:layout_height="91dp"-->
        <!--            android:layout_marginStart="150dp"-->
        <!--            android:layout_marginTop="25dp"-->
        <!--            app:layout_constraintBottom_toBottomOf="parent"-->
        <!--            tools:ignore="MissingConstraints"-->
        <!--            android:src="@mipmap/def" />-->

        <ImageView
            android:id="@+id/touxiang"
            android:layout_width="95dp"
            android:layout_height="91dp"
            android:layout_marginStart="140dp"
            android:layout_marginTop="15dp"
            android:src="@mipmap/iu1"
            tools:ignore="MissingConstraints" />

        <View
            android:id="@+id/image"
            android:layout_width="match_parent"
            android:layout_height="5px"
            android:layout_marginTop="140dp"
            android:background="#F5C8C4C5"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="100dp"
            tools:layout_editor_absoluteY="148dp" />

        <TextView
            android:id="@+id/editTextTextPersonName"
            android:layout_width="323dp"
            android:layout_height="41dp"
            android:layout_marginStart="60dp"
            android:layout_marginTop="210dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="用户设置"
            android:textColor="@color/black"
            android:textSize="50px"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="61dp"
            tools:layout_editor_absoluteY="254dp" />

        <View
            android:layout_width="wrap_content"
            android:layout_height="1dp"
            android:layout_below="@+id/editTextTextPersonName"
            android:layout_alignLeft="@+id/editTextTextPersonName"
            android:layout_alignRight="@+id/editTextTextPersonName"
            android:layout_marginLeft="2dp"
            android:layout_marginTop="0dp"
            android:background="#919393"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/editTextTextPersonName2"
            android:layout_width="323dp"
            android:layout_height="41dp"
            android:layout_marginStart="60dp"
            android:layout_marginTop="270dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="我的收藏"
            android:textColor="@color/black"
            android:textSize="50px"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="61dp"
            tools:layout_editor_absoluteY="254dp" />

        <View
            android:layout_width="wrap_content"
            android:layout_height="1dp"
            android:layout_below="@+id/editTextTextPersonName2"
            android:layout_alignLeft="@+id/editTextTextPersonName2"
            android:layout_alignRight="@+id/editTextTextPersonName2"
            android:layout_marginLeft="2dp"
            android:layout_marginTop="0dp"
            android:background="#919393"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/editTextTextPersonName3"
            android:layout_width="323dp"
            android:layout_height="41dp"
            android:layout_marginStart="60dp"
            android:layout_marginTop="320dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="关于"
            android:textColor="@color/black"
            android:textSize="50px"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="61dp"
            tools:layout_editor_absoluteY="254dp" />

        <View
            android:layout_width="wrap_content"
            android:layout_height="1dp"
            android:layout_below="@+id/editTextTextPersonName3"
            android:layout_alignLeft="@+id/editTextTextPersonName3"
            android:layout_alignRight="@+id/editTextTextPersonName3"
            android:layout_marginLeft="2dp"
            android:layout_marginTop="0dp"
            android:background="#919393"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/imagesetting"
            android:layout_width="26dp"
            android:layout_height="27dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="210dp"
            android:src="@drawable/ic_baseline_settings_24"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="18dp"
            tools:layout_editor_absoluteY="203dp" />

        <ImageView
            android:id="@+id/imageheart"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="270dp"
            android:src="@drawable/ic_baseline_favorite_24"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="18dp"
            tools:layout_editor_absoluteY="250dp" />

        <ImageView
            android:id="@+id/imagesmile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="320dp"
            android:src="@drawable/ic_baseline_insert_emoticon_24"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="324dp" />

        <ImageView
            android:id="@+id/imageleft"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="350dp"
            android:layout_marginTop="270dp"
            android:src="@drawable/ic_baseline_chevron_right_24"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="353dp"
            tools:layout_editor_absoluteY="203dp" />

        <ImageView
            android:id="@+id/imageleft2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="350dp"
            android:layout_marginTop="210dp"
            android:src="@drawable/ic_baseline_chevron_right_24"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="353dp"
            tools:layout_editor_absoluteY="260dp" />

        <ImageView
            android:id="@+id/imageleft3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="350dp"
            android:layout_marginTop="320dp"
            android:src="@drawable/ic_baseline_chevron_right_24"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="353dp"
            tools:layout_editor_absoluteY="320dp" />


        <TextView
            android:id="@+id/text_notifications"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="400dp"
            android:layout_marginEnd="8dp"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.665" />

    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>